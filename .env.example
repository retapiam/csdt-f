# ==================== CONFIGURACIÓN DE API ====================
# URL del backend Laravel
VITE_API_URL=http://localhost:8000/api

# Timeout de la API (en milisegundos)
VITE_API_TIMEOUT=60000

# ==================== CONFIGURACIÓN DE ALMACENAMIENTO ====================
# Claves para localStorage
VITE_TOKEN_KEY=csdt_token
VITE_USER_KEY=csdt_user

# ==================== CONFIGURACIÓN DE CACHE ====================
# Duración del cache (en minutos)
VITE_CACHE_DURATION=5

# Máximo de entradas en cache
VITE_CACHE_MAX_ENTRIES=100

# ==================== CONFIGURACIÓN DE APLICACIÓN ====================
# Nombre de la aplicación
VITE_APP_NAME=CSDT

# Entorno (development, production, staging)
VITE_APP_ENV=development

# Habilitar logs (true/false)
VITE_ENABLE_LOGS=true
